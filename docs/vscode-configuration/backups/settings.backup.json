{
  // ========================================
  // üéÆ NVIDIA GeForce RTX 2070 Max-Q GPU ACCELERATION
  // ========================================
  // GPU: RTX 2070 Max-Q (TU106, 8GB GDDR6, 1440 CUDA Cores)
  // Optimized for power efficiency and mobile performance
  // Terminal GPU Acceleration (CUDA-enabled)
  "terminal.integrated.gpuAcceleration": "on",
  "terminal.integrated.smoothScrolling": true,
  "terminal.integrated.enablePersistentSessions": true,
  "terminal.integrated.persistentSessionReviveProcess": "onExit",
  "terminal.integrated.defaultProfile.windows": "PowerShell",

  // Editor GPU Rendering (CUDA-optimized)
  "editor.smoothScrolling": true,
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorBlinking": "smooth",
  "editor.experimentalWhitespaceRendering": "off",
  "workbench.list.smoothScrolling": true,
  "workbench.tree.indent": 20,
  "workbench.tree.renderIndentGuides": "always",

  // Advanced GPU Features
  "window.experimental.windowControlsOverlay.enabled": true,
  "window.experimental.useTitleBarInactiveForegroundInSearchResults": true,
  "window.titleBarStyle": "custom",
  "window.menuBarVisibility": "compact",
  "window.commandCenter": true,
  "window.density.editorTabHeight": "default",

  // ========================================
  // üöÄ 64GB RAM OPTIMIZATION (HP OMEN)
  // ========================================
  // System: i7-9750H (6 cores, 12 threads), 64GB RAM, RTX 2070 Max-Q
  // Optimized for MAXIMUM PERFORMANCE
  "files.maxMemoryForLargeFilesMB": 32768,
  "search.maxFileSize": 209715200,

  // Maximum Memory Allocation (CUDA-aware) - INCREASED FOR 64GB
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/.next/**": true,
    "**/dist/**": true,
    "**/out/**": true,
    "**/.turbo/**": true,
    "**/coverage/**": true,
    "**/.cache/**": true,
    "**/.vercel/**": true,
    "**/.husky/**": true,
    "**/build/**": true
  },
  "search.followSymlinks": false,
  "search.smartCase": true,
  "search.maxResults": 100000,
  "search.quickOpen.includeSymbols": true,
  "search.quickOpen.includeHistory": true,

  // TypeScript Server Performance (64GB RAM optimized - DOUBLED)
  "typescript.tsserver.maxTsServerMemory": 65536,
  "typescript.tsserver.experimental.enableProjectDiagnostics": true,
  "typescript.tsserver.watchOptions": {
    "watchFile": "useFsEvents",
    "watchDirectory": "useFsEvents",
    "fallbackPolling": "dynamicPriorityPolling",
    "synchronousWatchDirectory": true,
    "excludeDirectories": ["**/node_modules", "**/.next", "**/.turbo"]
  },
  "typescript.disableAutomaticTypeAcquisition": false,
  "typescript.suggest.completeFunctionCalls": true,
  "typescript.suggest.includeCompletionsForImportStatements": true,
  "typescript.suggest.includeAutomaticOptionalChainCompletions": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.preferences.includePackageJsonAutoImports": "on",
  "typescript.preferences.jsxAttributeCompletionStyle": "auto",
  "typescript.preferences.importModuleSpecifier": "relative",

  // Editor Performance (GPU-Accelerated + 64GB RAM)
  "editor.semanticHighlighting.enabled": true,
  "editor.semanticTokenColorCustomizations": {},
  "editor.maxTokenizationLineLength": 50000,
  "editor.bracketPairColorization.enabled": true,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.guides.bracketPairs": "active",
  "editor.guides.bracketPairsHorizontal": "active",
  "editor.guides.highlightActiveBracketPair": true,
  "editor.guides.indentation": true,
  "editor.inlayHints.enabled": "onUnlessPressed",
  "editor.inlayHints.fontSize": 12,
  "editor.inlayHints.padding": true,
  "editor.suggest.preview": true,
  "editor.suggest.shareSuggestSelections": true,
  "editor.suggest.showIcons": true,
  "editor.suggest.showStatusBar": true,
  "editor.quickSuggestions": {
    "other": "on",
    "comments": "off",
    "strings": "on"
  },
  "editor.acceptSuggestionOnCommitCharacter": true,
  "editor.snippetSuggestions": "top",
  "editor.tabCompletion": "on",
  "editor.wordBasedSuggestions": "matchingDocuments",
  "editor.wordBasedSuggestionsMode": "matchingDocuments",
  "editor.renderWhitespace": "selection",
  "editor.renderControlCharacters": false,
  "editor.renderLineHighlight": "all",
  "editor.renderLineHighlightOnlyWhenFocus": false,
  "editor.minimap.enabled": true,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.maxColumn": 120,
  "editor.minimap.showSlider": "always",
  "editor.minimap.side": "right",
  "editor.stickyScroll.enabled": true,
  "editor.stickyScroll.maxLineCount": 5,
  "editor.unicodeHighlight.ambiguousCharacters": true,
  "editor.unicodeHighlight.invisibleCharacters": true,

  // ========================================
  // üìù EDITOR CONFIGURATION
  // ========================================
  "editor.fontFamily": "'Fira Code', 'Cascadia Code', 'JetBrains Mono', 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.lineHeight": 22,
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.detectIndentation": true,
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "explicit"
  },

  // ========================================
  // üé® VISUAL SETTINGS & UI OPTIMIZATION
  // ========================================
  "workbench.colorTheme": "Default Dark Modern",
  "workbench.iconTheme": "vs-seti",
  "workbench.startupEditor": "none",
  "workbench.editor.enablePreview": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.value": 15,
  "workbench.editor.tabCloseButton": "right",
  "workbench.editor.tabSizing": "fit",
  "workbench.activityBar.location": "top",
  "workbench.statusBar.visible": true,
  "workbench.layoutControl.enabled": true,

  // ========================================
  // üìÅ FILE MANAGEMENT
  // ========================================
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "files.autoSave": "off",
  "files.eol": "\n",
  "files.associations": {
    "*.css": "tailwindcss",
    "*.prisma": "prisma",
    "*.env*": "dotenv",
    "*.mjs": "javascript",
    "*.cjs": "javascript",
    "*.mdx": "markdown",
    "tsconfig*.json": "jsonc"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "**/dist": true,
    "**/.next": true,
    "**/out": true,
    "**/.turbo": true,
    "**/coverage": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/dist": true,
    "**/.next": true,
    "**/out": true,
    "**/.turbo": true,
    "**/coverage": true,
    "**/package-lock.json": true,
    "**/yarn.lock": true,
    "**/pnpm-lock.yaml": true
  },

  // ========================================
  // üîß LANGUAGE SPECIFIC
  // ========================================
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.wordWrap": "on",
    "editor.quickSuggestions": {
      "comments": "on",
      "strings": "on",
      "other": "on"
    }
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },

  // ========================================
  // ‚úÖ ESLINT & LINTING
  // ========================================
  "eslint.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "eslint.run": "onType",

  // ========================================
  // üß™ GIT INTEGRATION
  // ========================================
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.ignoreLimitWarning": true,

  // ========================================
  // üîç TAILWIND CSS
  // ========================================
  "tailwindCSS.experimental.classRegex": [
    ["cva\\(([^)]*)\\)", "[\"'`]([^\"'`]*).*?[\"'`]"],
    ["cx\\(([^)]*)\\)", "(?:'|\"|`)([^']*)(?:'|\"|`)"]
  ],
  "tailwindCSS.includeLanguages": {
    "typescript": "javascript",
    "typescriptreact": "javascript"
  },

  // ========================================
  // üöÄ NEXT.JS OPTIMIZATION
  // ========================================
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.suggest.autoImports": true,
  "javascript.format.semicolons": "insert",

  // ========================================
  // üéØ PRISMA SETTINGS
  // ========================================
  "prisma.showPrismaDataPlatformNotification": false,

  // ========================================
  // üß† AI COPILOT SETTINGS (CUDA-ACCELERATED)
  // ========================================
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": true,
    "scminput": false,
    "github.copilot.panel.enable": true,
    "yaml": true
  },
  "github.copilot.editor.enableAutoCompletions": true,
  "github.copilot.editor.enableCodeActions": true,
  "github.copilot.renameSuggestions.triggerAutomatically": true,

  // ========================================
  // ü§ñ GITHUB COPILOT CHAT (AI PAIR PROGRAMMING)
  // ========================================
  "github.copilot.chat.localeOverride": "en",
  "github.copilot.chat.terminalChatLocation": "quickChat",
  "github.copilot.chat.scopeSelection": true,
  "github.copilot.chat.useProjectTemplates": true,
  "github.copilot.chat.followUps": "on",
  "github.copilot.chat.search.semanticTextResults": true,
  "github.copilot.chat.search.codeSearchResults": true,
  "github.copilot.chat.experimental.generateDocs.enabled": true,

  // ========================================
  // üîÆ INTELLISENSE & AI SUGGESTIONS
  // ========================================
  "editor.inlineSuggest.enabled": true,
  "editor.inlineSuggest.showToolbar": "always",
  "editor.inlineSuggest.suppressSuggestions": false,
  "editor.suggestSelection": "first",
  "editor.suggest.localityBonus": true,
  "editor.suggest.insertMode": "replace",
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.parameterHints.enabled": true,
  "editor.hover.enabled": true,
  "editor.hover.sticky": true,
  "editor.hover.delay": 300,

  // ========================================
  // üé™ EMMET
  // ========================================
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "typescript": "typescriptreact"
  },
  "emmet.triggerExpansionOnTab": true,

  // ========================================
  // üß™ TESTING & DEBUGGING (CUDA-OPTIMIZED)
  // ========================================
  "jest.runMode": "on-demand",
  "jest.coverageFormatter": "DefaultFormatter",
  "jest.outputConfig": {
    "revealOutput": "on-exec",
    "revealWithFocus": "terminal",
    "clearOnRun": "terminal"
  },
  "testing.automaticallyOpenPeekView": "failureInVisibleDocument",
  "testing.openTesting": "neverOpen",
  "debug.internalConsoleOptions": "neverOpen",
  "debug.openDebug": "neverOpen",
  "debug.showBreakpointsInOverviewRuler": true,
  "debug.console.fontSize": 14,
  "debug.console.lineHeight": 22,

  // ========================================
  // üéØ CODE ACTIONS & REFACTORING
  // ========================================
  "editor.lightbulb.enabled": "on",
  "editor.codeActionWidget.includeNearbyQuickFixes": true,
  "editor.codeActionWidget.showHeaders": true,
  "editor.quickSuggestionsDelay": 10,
  "editor.rename.enablePreview": true,
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",
  "editor.gotoLocation.multipleDeclarations": "goto",
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleReferences": "goto",

  // ========================================
  // üöÄ NEXT.JS & REACT OPTIMIZATION
  // ========================================
  "javascript.inlayHints.enumMemberValues.enabled": true,
  "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
  "javascript.inlayHints.parameterNames.enabled": "all",
  "javascript.inlayHints.parameterTypes.enabled": true,
  "javascript.inlayHints.propertyDeclarationTypes.enabled": true,
  "javascript.inlayHints.variableTypes.enabled": true,
  "typescript.inlayHints.enumMemberValues.enabled": true,
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.inlayHints.variableTypes.enabled": true,

  // ========================================
  // üîç ADVANCED SEARCH & SYMBOLS
  // ========================================
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.quickOpen.preserveInput": false,
  "search.collapseResults": "alwaysCollapse",
  "search.searchEditor.defaultNumberOfContextLines": 1,
  "search.seedWithNearestWord": true,
  "search.useGlobalIgnoreFiles": true,
  "search.useParentIgnoreFiles": true,

  // ========================================
  // üíæ AUTO-SAVE & FILE MANAGEMENT
  // ========================================
  "files.autoSaveDelay": 1000,
  "files.hotExit": "onExitAndWindowClose",
  "files.defaultLanguage": "typescript",
  "files.refactoring.autoSave": true,

  // ========================================
  // üé® BRACKET PAIR GUIDES & VISUAL AIDS
  // ========================================
  "workbench.colorCustomizations": {
    "editorBracketHighlight.foreground1": "#FFD700",
    "editorBracketHighlight.foreground2": "#DA70D6",
    "editorBracketHighlight.foreground3": "#87CEFA",
    "editorBracketHighlight.foreground4": "#FA8072",
    "editorBracketHighlight.foreground5": "#7CFC00",
    "editorBracketHighlight.foreground6": "#FF69B4",
    "editorBracketHighlight.unexpectedBracket.foreground": "#FF0000"
  },

  // ========================================
  // üîî NOTIFICATIONS & TELEMETRY
  // ========================================
  "extensions.ignoreRecommendations": false,
  "extensions.autoUpdate": true,
  "extensions.autoCheckUpdates": true,
  "update.mode": "start",
  "update.showReleaseNotes": true,
  "telemetry.telemetryLevel": "off",
  "workbench.enableExperiments": true,

  // ========================================
  // üåê PATH INTELLISENSE & IMPORTS
  // ========================================
  "path-intellisense.autoSlashAfterDirectory": true,
  "path-intellisense.extensionOnImport": true,
  "path-intellisense.showHiddenFiles": true,

  // ========================================
  // üîß TERMINAL ENHANCEMENTS
  // ========================================
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.fontFamily": "'Cascadia Code', 'Fira Code', monospace",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.rightClickBehavior": "default",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.shellIntegration.enabled": true,
  "terminal.integrated.shellIntegration.decorationsEnabled": "both",
  "terminal.integrated.tabs.enabled": true,
  "terminal.integrated.tabs.location": "right",
  "terminal.integrated.environmentChangesIndicator": "on",
  "terminal.integrated.persistentSessionScrollback": 1000,
  "terminal.integrated.cwd": "${workspaceFolder}/farmers-market",
  "terminal.integrated.defaultLocation": "view",

  // ========================================
  // üéØ AGRICULTURAL PROJECT SPECIFIC
  // ========================================
  "css.lint.unknownAtRules": "ignore",
  "scss.lint.unknownAtRules": "ignore",
  "tailwindCSS.emmetCompletions": true,
  "tailwindCSS.hovers": true,
  "tailwindCSS.suggestions": true,
  "tailwindCSS.validate": true,
  "tailwindCSS.colorDecorators": true,
  "tailwindCSS.lint.cssConflict": "warning",
  "tailwindCSS.lint.invalidApply": "error",
  "tailwindCSS.lint.invalidScreen": "error",
  "tailwindCSS.lint.invalidVariant": "error",
  "tailwindCSS.lint.invalidConfigPath": "error",
  "tailwindCSS.lint.invalidTailwindDirective": "error",
  "tailwindCSS.lint.recommendedVariantOrder": "warning",

  // ========================================
  // üöÄ PRODUCTIVITY BOOSTERS
  // ========================================
  "editor.accessibilitySupport": "off",
  "editor.find.addExtraSpaceOnTop": true,
  "editor.find.autoFindInSelection": "multiline",
  "editor.find.seedSearchStringFromSelection": "selection",
  "editor.linkedEditing": true,
  "editor.matchBrackets": "always",
  "editor.occurrencesHighlight": "multiFile",
  "editor.selectionHighlight": true,
  "editor.wordWrap": "on",
  "editor.wrappingIndent": "indent",

  // ========================================
  // üé® COLOR & ICON THEME OPTIMIZATION
  // ========================================
  "workbench.productIconTheme": "Default",
  "workbench.preferredDarkColorTheme": "Default Dark Modern",
  "workbench.preferredLightColorTheme": "Default Light Modern",
  "workbench.preferredHighContrastColorTheme": "Default High Contrast",
  "workbench.preferredHighContrastLightColorTheme": "Default High Contrast Light",
  "workbench.editor.tabActionLocation": "right",
  "testing.automaticallyOpenTestResults": "neverOpen",
  "github.copilot.chat.editor.temporalContext.enabled": true,
  "[chatmode]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "editor.aiStats.enabled": true,
  "cSpell.words": ["Nsight"]
}
