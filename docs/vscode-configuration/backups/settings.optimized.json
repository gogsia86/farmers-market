{
  // ============================================================================
  // HP OMEN DEVELOPMENT WORKSTATION - OPTIMIZED SETTINGS
  // ============================================================================
  // Hardware: i7-9750H (6 cores, 12 threads @ 2.60GHz)
  //           64GB DDR4 RAM
  //           NVIDIA GeForce RTX 2070 Max-Q (8GB GDDR6, 2304 CUDA cores)
  // OS:       Windows 11 Pro Build 26200
  // Updated:  October 21, 2025
  // Purpose:  Maximum performance for Farmers Market Platform development
  //
  // Related Files:
  // - .vscode/tasks.json (Build & development tasks)
  // - .vscode/launch.json (Debugging configurations)
  // - .vscode/extensions.json (Recommended extensions)
  // - farmers-market/next.config.js (Next.js optimization)
  // - farmers-market/package.json (Performance scripts)
  // - farmers-market/.env.performance (Environment variables)
  // ============================================================================

  // ============================================================================
  // SECTION 1: HARDWARE ACCELERATION (GPU + CPU)
  // ============================================================================
  // RTX 2070 Max-Q GPU acceleration for rendering and terminal
  // Enables smooth scrolling, animations, and CUDA-optimized operations
  // ============================================================================

  // GPU-Accelerated Terminal (2304 CUDA cores)
  "terminal.integrated.gpuAcceleration": "on",
  "terminal.integrated.smoothScrolling": true,
  "terminal.integrated.enablePersistentSessions": true,
  "terminal.integrated.persistentSessionReviveProcess": "onExit",
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.fontFamily": "'Cascadia Code', 'Fira Code', monospace",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.rightClickBehavior": "default",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.shellIntegration.enabled": true,
  "terminal.integrated.shellIntegration.decorationsEnabled": "both",
  "terminal.integrated.tabs.enabled": true,
  "terminal.integrated.tabs.location": "right",
  "terminal.integrated.environmentChangesIndicator": "on",
  "terminal.integrated.persistentSessionScrollback": 1000,
  "terminal.integrated.cwd": "${workspaceFolder}/farmers-market",
  "terminal.integrated.defaultLocation": "view",

  // GPU-Accelerated Editor Rendering
  "editor.smoothScrolling": true,
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorBlinking": "smooth",
  "editor.experimentalWhitespaceRendering": "off",
  "workbench.list.smoothScrolling": true,
  "workbench.tree.indent": 20,
  "workbench.tree.renderIndentGuides": "always",

  // GPU-Accelerated Window Features
  "window.experimental.windowControlsOverlay.enabled": true,
  "window.experimental.useTitleBarInactiveForegroundInSearchResults": true,
  "window.titleBarStyle": "custom",
  "window.menuBarVisibility": "compact",
  "window.commandCenter": true,
  "window.density.editorTabHeight": "default",

  // ============================================================================
  // SECTION 2: MEMORY OPTIMIZATION (64GB RAM)
  // ============================================================================
  // Optimized for 64GB RAM system - aggressive memory allocation
  // TypeScript Server: 65GB max (can use full RAM if needed)
  // File Operations: 32GB for large file handling
  // Search: 100K results (2x default)
  // ============================================================================

  // File System Memory (32GB allocation)
  "files.maxMemoryForLargeFilesMB": 32768,
  "search.maxFileSize": 209715200,
  "files.autoSaveDelay": 1000,
  "files.hotExit": "onExitAndWindowClose",
  "files.defaultLanguage": "typescript",
  "files.refactoring.autoSave": true,
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "files.autoSave": "off",
  "files.eol": "\n",

  // File Watcher Exclusions (Performance optimization)
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/.next/**": true,
    "**/dist/**": true,
    "**/out/**": true,
    "**/.turbo/**": true,
    "**/coverage/**": true,
    "**/.cache/**": true,
    "**/.vercel/**": true,
    "**/.husky/**": true,
    "**/build/**": true
  },

  // Search Performance (100K results, 200MB max file size)
  "search.followSymlinks": false,
  "search.smartCase": true,
  "search.maxResults": 100000,
  "search.quickOpen.includeSymbols": true,
  "search.quickOpen.includeHistory": true,
  "search.collapseResults": "alwaysCollapse",
  "search.searchEditor.defaultNumberOfContextLines": 1,
  "search.seedWithNearestWord": true,
  "search.useGlobalIgnoreFiles": true,
  "search.useParentIgnoreFiles": true,
  "search.exclude": {
    "**/node_modules": true,
    "**/dist": true,
    "**/.next": true,
    "**/out": true,
    "**/.turbo": true,
    "**/coverage": true,
    "**/package-lock.json": true,
    "**/yarn.lock": true,
    "**/pnpm-lock.yaml": true
  },

  // TypeScript Server (65GB max memory - can use full RAM)
  "typescript.tsserver.maxTsServerMemory": 65536,
  "typescript.tsserver.experimental.enableProjectDiagnostics": true,
  "typescript.tsserver.watchOptions": {
    "watchFile": "useFsEvents",
    "watchDirectory": "useFsEvents",
    "fallbackPolling": "dynamicPriorityPolling",
    "synchronousWatchDirectory": true,
    "excludeDirectories": ["**/node_modules", "**/.next", "**/.turbo"]
  },
  "typescript.disableAutomaticTypeAcquisition": false,
  "typescript.suggest.completeFunctionCalls": true,
  "typescript.suggest.includeCompletionsForImportStatements": true,
  "typescript.suggest.includeAutomaticOptionalChainCompletions": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.preferences.includePackageJsonAutoImports": "on",
  "typescript.preferences.jsxAttributeCompletionStyle": "auto",
  "typescript.preferences.importModuleSpecifier": "relative",

  // TypeScript Inlay Hints (Maximum type information)
  "typescript.inlayHints.enumMemberValues.enabled": true,
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.inlayHints.variableTypes.enabled": true,

  // JavaScript Performance
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.suggest.autoImports": true,
  "javascript.format.semicolons": "insert",
  "javascript.inlayHints.enumMemberValues.enabled": true,
  "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
  "javascript.inlayHints.parameterNames.enabled": "all",
  "javascript.inlayHints.parameterTypes.enabled": true,
  "javascript.inlayHints.propertyDeclarationTypes.enabled": true,
  "javascript.inlayHints.variableTypes.enabled": true,

  // ============================================================================
  // SECTION 3: EDITOR PERFORMANCE & FEATURES
  // ============================================================================
  // Optimized for development speed and code quality
  // Semantic highlighting, bracket colorization, inlay hints all enabled
  // ============================================================================

  // Editor Visual Performance
  "editor.semanticHighlighting.enabled": true,
  "editor.semanticTokenColorCustomizations": {},
  "editor.maxTokenizationLineLength": 50000,
  "editor.bracketPairColorization.enabled": true,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.guides.bracketPairs": "active",
  "editor.guides.bracketPairsHorizontal": "active",
  "editor.guides.highlightActiveBracketPair": true,
  "editor.guides.indentation": true,

  // Editor Suggestions & IntelliSense
  "editor.inlayHints.enabled": "onUnlessPressed",
  "editor.inlayHints.fontSize": 12,
  "editor.inlayHints.padding": true,
  "editor.suggest.preview": true,
  "editor.suggest.shareSuggestSelections": true,
  "editor.suggest.showIcons": true,
  "editor.suggest.showStatusBar": true,
  "editor.suggest.localityBonus": true,
  "editor.suggest.insertMode": "replace",
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.suggestSelection": "first",
  "editor.quickSuggestions": {
    "other": "on",
    "comments": "off",
    "strings": "on"
  },
  "editor.quickSuggestionsDelay": 10,
  "editor.acceptSuggestionOnCommitCharacter": true,
  "editor.snippetSuggestions": "top",
  "editor.tabCompletion": "on",
  "editor.wordBasedSuggestions": "matchingDocuments",
  "editor.wordBasedSuggestionsMode": "matchingDocuments",

  // Editor Navigation & Parameters
  "editor.parameterHints.enabled": true,
  "editor.hover.enabled": true,
  "editor.hover.sticky": true,
  "editor.hover.delay": 300,
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",
  "editor.gotoLocation.multipleDeclarations": "goto",
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleReferences": "goto",

  // Editor Visual Aids
  "editor.renderWhitespace": "selection",
  "editor.renderControlCharacters": false,
  "editor.renderLineHighlight": "all",
  "editor.renderLineHighlightOnlyWhenFocus": false,
  "editor.minimap.enabled": true,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.maxColumn": 120,
  "editor.minimap.showSlider": "always",
  "editor.minimap.side": "right",
  "editor.stickyScroll.enabled": true,
  "editor.stickyScroll.maxLineCount": 5,
  "editor.unicodeHighlight.ambiguousCharacters": true,
  "editor.unicodeHighlight.invisibleCharacters": true,

  // Editor Code Actions & Refactoring
  "editor.lightbulb.enabled": "on",
  "editor.codeActionWidget.includeNearbyQuickFixes": true,
  "editor.codeActionWidget.showHeaders": true,
  "editor.rename.enablePreview": true,

  // Editor Productivity Features
  "editor.accessibilitySupport": "off",
  "editor.find.addExtraSpaceOnTop": true,
  "editor.find.autoFindInSelection": "multiline",
  "editor.find.seedSearchStringFromSelection": "selection",
  "editor.linkedEditing": true,
  "editor.matchBrackets": "always",
  "editor.occurrencesHighlight": "multiFile",
  "editor.selectionHighlight": true,
  "editor.wordWrap": "on",
  "editor.wrappingIndent": "indent",

  // ============================================================================
  // SECTION 4: CODE FORMATTING & QUALITY
  // ============================================================================
  // Prettier for formatting, ESLint for linting
  // Format on save, fix on save enabled for maximum code quality
  // ============================================================================

  // Editor Font & Layout
  "editor.fontFamily": "'Fira Code', 'Cascadia Code', 'JetBrains Mono', 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.lineHeight": 22,
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.detectIndentation": true,

  // Formatting Configuration
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "explicit"
  },

  // ESLint Configuration
  "eslint.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "eslint.run": "onType",

  // ============================================================================
  // SECTION 5: AI-POWERED DEVELOPMENT (GitHub Copilot)
  // ============================================================================
  // Full Copilot integration with all features enabled
  // CUDA acceleration for AI suggestions
  // ============================================================================

  // Copilot Core Settings
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": true,
    "scminput": false,
    "github.copilot.panel.enable": true,
    "yaml": true
  },
  "github.copilot.editor.enableAutoCompletions": true,
  "github.copilot.editor.enableCodeActions": true,
  "github.copilot.renameSuggestions.triggerAutomatically": true,

  // Copilot Chat Configuration
  "github.copilot.chat.localeOverride": "en",
  "github.copilot.chat.terminalChatLocation": "quickChat",
  "github.copilot.chat.scopeSelection": true,
  "github.copilot.chat.useProjectTemplates": true,
  "github.copilot.chat.followUps": "on",
  "github.copilot.chat.search.semanticTextResults": true,
  "github.copilot.chat.search.codeSearchResults": true,
  "github.copilot.chat.experimental.generateDocs.enabled": true,
  "github.copilot.chat.editor.temporalContext.enabled": true,

  // Inline Suggestions
  "editor.inlineSuggest.enabled": true,
  "editor.inlineSuggest.showToolbar": "always",
  "editor.inlineSuggest.suppressSuggestions": false,
  "editor.aiStats.enabled": true,

  // ============================================================================
  // SECTION 6: FRAMEWORK-SPECIFIC CONFIGURATION
  // ============================================================================
  // Next.js, React, Tailwind CSS, Prisma optimizations
  // ============================================================================

  // File Associations
  "files.associations": {
    "*.css": "tailwindcss",
    "*.prisma": "prisma",
    "*.env*": "dotenv",
    "*.mjs": "javascript",
    "*.cjs": "javascript",
    "*.mdx": "markdown",
    "tsconfig*.json": "jsonc"
  },

  // Files to Exclude from Explorer
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "**/dist": true,
    "**/.next": true,
    "**/out": true,
    "**/.turbo": true,
    "**/coverage": true
  },

  // Language-Specific Formatters
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.wordWrap": "on",
    "editor.quickSuggestions": {
      "comments": "on",
      "strings": "on",
      "other": "on"
    }
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "[chatmode]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },

  // Tailwind CSS Configuration
  "tailwindCSS.experimental.classRegex": [
    ["cva\\(([^)]*)\\)", "[\"'`]([^\"'`]*).*?[\"'`]"],
    ["cx\\(([^)]*)\\)", "(?:'|\"|`)([^']*)(?:'|\"|`)"]
  ],
  "tailwindCSS.includeLanguages": {
    "typescript": "javascript",
    "typescriptreact": "javascript"
  },
  "tailwindCSS.emmetCompletions": true,
  "tailwindCSS.hovers": true,
  "tailwindCSS.suggestions": true,
  "tailwindCSS.validate": true,
  "tailwindCSS.colorDecorators": true,
  "tailwindCSS.lint.cssConflict": "warning",
  "tailwindCSS.lint.invalidApply": "error",
  "tailwindCSS.lint.invalidScreen": "error",
  "tailwindCSS.lint.invalidVariant": "error",
  "tailwindCSS.lint.invalidConfigPath": "error",
  "tailwindCSS.lint.invalidTailwindDirective": "error",
  "tailwindCSS.lint.recommendedVariantOrder": "warning",

  // CSS/SCSS Linting
  "css.lint.unknownAtRules": "ignore",
  "scss.lint.unknownAtRules": "ignore",

  // Prisma Configuration
  "prisma.showPrismaDataPlatformNotification": false,

  // Emmet Configuration
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "typescript": "typescriptreact"
  },
  "emmet.triggerExpansionOnTab": true,

  // ============================================================================
  // SECTION 7: TESTING & DEBUGGING
  // ============================================================================
  // Jest, Testing Library, and debugging optimizations
  // ============================================================================

  // Jest Configuration
  "jest.runMode": "on-demand",
  "jest.coverageFormatter": "DefaultFormatter",
  "jest.outputConfig": {
    "revealOutput": "on-exec",
    "revealWithFocus": "terminal",
    "clearOnRun": "terminal"
  },

  // Testing Configuration
  "testing.automaticallyOpenPeekView": "failureInVisibleDocument",
  "testing.openTesting": "neverOpen",
  "testing.automaticallyOpenTestResults": "neverOpen",

  // Debug Configuration
  "debug.internalConsoleOptions": "neverOpen",
  "debug.openDebug": "neverOpen",
  "debug.showBreakpointsInOverviewRuler": true,
  "debug.console.fontSize": 14,
  "debug.console.lineHeight": 22,

  // ============================================================================
  // SECTION 8: GIT INTEGRATION
  // ============================================================================
  // Git auto-fetch, smart commit, and performance optimizations
  // ============================================================================

  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.ignoreLimitWarning": true,

  // ============================================================================
  // SECTION 9: UI & VISUAL CUSTOMIZATION
  // ============================================================================
  // Theme, layout, and visual preferences
  // ============================================================================

  // Workbench Theme & Icons
  "workbench.colorTheme": "Default Dark Modern",
  "workbench.iconTheme": "vs-seti",
  "workbench.productIconTheme": "Default",
  "workbench.preferredDarkColorTheme": "Default Dark Modern",
  "workbench.preferredLightColorTheme": "Default Light Modern",
  "workbench.preferredHighContrastColorTheme": "Default High Contrast",
  "workbench.preferredHighContrastLightColorTheme": "Default High Contrast Light",

  // Workbench Layout
  "workbench.startupEditor": "none",
  "workbench.editor.enablePreview": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.value": 15,
  "workbench.editor.tabCloseButton": "right",
  "workbench.editor.tabSizing": "fit",
  "workbench.editor.tabActionLocation": "right",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.activityBar.location": "top",
  "workbench.statusBar.visible": true,
  "workbench.layoutControl.enabled": true,
  "workbench.quickOpen.preserveInput": false,

  // Bracket Pair Color Customization
  "workbench.colorCustomizations": {
    "editorBracketHighlight.foreground1": "#FFD700",
    "editorBracketHighlight.foreground2": "#DA70D6",
    "editorBracketHighlight.foreground3": "#87CEFA",
    "editorBracketHighlight.foreground4": "#FA8072",
    "editorBracketHighlight.foreground5": "#7CFC00",
    "editorBracketHighlight.foreground6": "#FF69B4",
    "editorBracketHighlight.unexpectedBracket.foreground": "#FF0000"
  },

  // ============================================================================
  // SECTION 10: EXTENSIONS & UPDATES
  // ============================================================================
  // Extension management and update preferences
  // ============================================================================

  "extensions.ignoreRecommendations": false,
  "extensions.autoUpdate": true,
  "extensions.autoCheckUpdates": true,
  "update.mode": "start",
  "update.showReleaseNotes": true,
  "telemetry.telemetryLevel": "off",
  "workbench.enableExperiments": true,

  // ============================================================================
  // SECTION 11: PATH INTELLISENSE & IMPORTS
  // ============================================================================
  // Auto-completion for file paths and imports
  // ============================================================================

  "path-intellisense.autoSlashAfterDirectory": true,
  "path-intellisense.extensionOnImport": true,
  "path-intellisense.showHiddenFiles": true,

  // ============================================================================
  // SECTION 12: SPELL CHECKING
  // ============================================================================
  // Custom dictionary for project-specific terms
  // ============================================================================

  "cSpell.words": ["Nsight", "Farmers", "OMEN", "Copilot", "Prisma"]

  // ============================================================================
  // END OF OPTIMIZED SETTINGS
  // ============================================================================
  // To activate: Rename to settings.json or merge with existing settings.json
  // Performance Test: npm run dev:turbo (should start in 3-5 seconds)
  // Documentation: See .vscode/SETTINGS_ANALYSIS_AND_OPTIMIZATION.md
  // ============================================================================
}
