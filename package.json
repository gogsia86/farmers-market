{
  "name": "farmers-market",
  "version": "1.0.0",
  "private": true,
  "description": "Divine Agricultural E-Commerce Platform",
  "engines": {
    "node": ">=20.19.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "postinstall": "prisma generate || true",
    "prepare": "husky install || true",
    "dev": "cross-env NODE_OPTIONS='--max-old-space-size=16384' next dev -p 3001",
    "dev:safe": "node scripts/start-dev-safe.js",
    "dev:turbo": "cross-env NODE_OPTIONS='--max-old-space-size=16384' next dev --turbo -p 3001",
    "dev:omen": "cross-env NODE_OPTIONS='--max-old-space-size=32768 --max-semi-space-size=512' next dev --turbo -p 3001",
    "build": "cross-env NODE_OPTIONS='--max-old-space-size=16384' prisma generate && next build",
    "build:docker": "prisma generate && cross-env SKIP_ENV_VALIDATION=true next build",
    "build:optimized": "cross-env NODE_OPTIONS='--max-old-space-size=16384' prisma generate && next build",
    "build:omen": "cross-env NODE_OPTIONS='--max-old-space-size=32768 --max-semi-space-size=512' prisma generate && next build",
    "build:analyze": "cross-env ANALYZE=true NODE_OPTIONS='--max-old-space-size=16384' next build",
    "start": "cross-env NODE_OPTIONS='--max-old-space-size=8192' next start -p 3001",
    "start:omen": "cross-env NODE_OPTIONS='--max-old-space-size=16384' next start -p 3001",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:quiet": "next lint --quiet",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "quality": "npm run type-check && npm run lint && npm run format:check",
    "quality:fix": "npm run type-check && npm run lint:fix && npm run format",
    "quality:omen": "npm run type-check:omen && npm run lint && npm run format:check",
    "prebuild": "npm run quality",
    "prebuild:optimized": "npm run quality",
    "prebuild:omen": "npm run quality:omen",
    "test": "cross-env NODE_OPTIONS=--max-old-space-size=8192 jest --maxWorkers=6",
    "test:watch": "cross-env NODE_OPTIONS=--max-old-space-size=8192 jest --watch --maxWorkers=4",
    "test:coverage": "cross-env NODE_OPTIONS=--max-old-space-size=8192 jest --coverage --maxWorkers=6",
    "test:omen": "cross-env NODE_OPTIONS=--max-old-space-size=16384 jest --maxWorkers=10",
    "test:e2e": "node scripts/e2e-test.js --workers=6",
    "test:e2e:ui": "node scripts/e2e-test.js --ui",
    "test:e2e:headed": "node scripts/e2e-test.js --headed --workers=4",
    "test:e2e:debug": "node scripts/e2e-test.js --debug",
    "test:e2e:omen": "node scripts/e2e-test.js --workers=10",
    "test:e2e:direct": "playwright test --workers=6",
    "test:all": "npm run test && npm run test:e2e",
    "test:all:omen": "npm run test:omen && npm run test:e2e:omen",
    "test:cursorrules": "tsx scripts/test-cursorrules-compliance.ts",
    "test:perplexity": "tsx scripts/test-perplexity.ts",
    "type-check": "tsc --noEmit",
    "type-check:omen": "tsc --noEmit --maxNodeModuleJsDepth 10",
    "kill-server": "node scripts/kill-dev-server.js",
    "db:migrate": "prisma migrate dev",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "db:test:setup": "tsx scripts/setup-test-db.ts",
    "vercel-build": "prisma generate && next build"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.2",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.67.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.208.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-node": "^0.208.0",
    "@opentelemetry/semantic-conventions": "^1.38.0",
    "@prisma/client": "^7.0.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-toast": "^1.2.15",
    "@sentry/nextjs": "^10.26.0",
    "@stripe/react-stripe-js": "^5.4.0",
    "@stripe/stripe-js": "^8.5.2",
    "@tanstack/react-query": "^5.90.10",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-backend-webgl": "^4.22.0",
    "@tensorflow/tfjs-node-gpu": "^4.22.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "bcryptjs": "^3.0.3",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.24",
    "jose": "^6.1.2",
    "lucide-react": "^0.554.0",
    "next": "16.0.3",
    "next-auth": "^5.0.0-beta.30",
    "next-intl": "^4.5.5",
    "next-themes": "^0.4.6",
    "nodemailer": "^7.0.10",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.66.1",
    "sharp": "^0.34.5",
    "sonner": "^2.0.7",
    "stripe": "^20.0.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "16.0.3",
    "@playwright/test": "^1.56.1",
    "@swc/core": "^1.15.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/nodemailer": "^7.0.4",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "critters": "^0.0.25",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.0.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "ioredis": "^5.8.2",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^27.2.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "prisma": "^7.0.0",
    "tailwindcss": "^3.4.18",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "5.9.3"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "overrides": {
    "glob": "^10.3.10",
    "inflight": "npm:noop2@^2.0.0",
    "js-yaml": "^4.1.1"
  }
}
