{
  "name": "farmers-market",
  "version": "1.0.0",
  "private": true,
  "description": "Divine Agricultural E-Commerce Platform - Prisma 7",
  "engines": {
    "node": ">=20.19.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "postinstall": "prisma generate || true",
    "prepare": "husky",
    "dev": "cross-env NODE_OPTIONS='--max-old-space-size=16384' NODE_ENV=development next dev --turbo -p 3001",
    "dev:safe": "node scripts/start-dev-safe.js",
    "dev:webpack": "cross-env NODE_OPTIONS='--max-old-space-size=16384' NODE_ENV=development next dev -p 3001",
    "dev:turbo": "cross-env NODE_OPTIONS='--max-old-space-size=16384' NODE_ENV=development next dev --turbo -p 3001",
    "dev:omen": "cross-env NODE_OPTIONS='--max-old-space-size=32768 --max-semi-space-size=512' NODE_ENV=development next dev --turbo -p 3001",
    "dev:logger": "cross-env NODE_OPTIONS='--max-old-space-size=16384' NODE_ENV=development LOG_LEVEL=debug next dev --turbo -p 3001",
    "build": "cross-env NODE_OPTIONS='--max-old-space-size=16384' prisma generate && next build",
    "build:docker": "prisma generate && cross-env SKIP_ENV_VALIDATION=true next build",
    "build:optimized": "cross-env NODE_OPTIONS='--max-old-space-size=16384' prisma generate && next build",
    "build:omen": "cross-env NODE_OPTIONS='--max-old-space-size=32768 --max-semi-space-size=512' prisma generate && next build",
    "build:analyze": "cross-env ANALYZE=true NODE_OPTIONS='--max-old-space-size=16384' next build --webpack",
    "bundle:measure": "node scripts/measure-bundle-performance.mjs",
    "bundle:check": "npm run build:analyze && npm run bundle:measure",
    "bundle:validate": "npm run bundle:check",
    "start": "cross-env NODE_OPTIONS='--max-old-space-size=8192' next start -p 3001",
    "start:omen": "cross-env NODE_OPTIONS='--max-old-space-size=16384' next start -p 3001",
    "clean:cache": "rimraf .jest-cache coverage playwright-report",
    "clean:all": "rimraf .jest-cache coverage playwright-report node_modules/.cache",
    "cleanup:phase1": "bash scripts/cleanup-phase1.sh",
    "cleanup:phase2": "bash scripts/phase2-migrate-logging.sh",
    "audit:console": "bash scripts/audit-console-logs.sh",
    "audit:todo": "bash scripts/generate-todo-inventory.sh",
    "validate:platform": "tsx scripts/validate-platform.ts",
    "validate:errors": "tsx scripts/detect-errors.ts",
    "validate:quick": "npx tsc --noEmit && npm test",
    "validate:all": "npm run validate:platform && npm run validate:errors && npm run type-check",
    "fix:quick": "tsx scripts/quick-fix.ts --all",
    "fix:quick:dry": "tsx scripts/quick-fix.ts --all --dry-run",
    "fix:logger": "tsx scripts/quick-fix.ts logger",
    "fix:auth": "tsx scripts/fix-nextauth.ts",
    "fix:email": "tsx scripts/quick-fix.ts email",
    "fix:imports": "tsx scripts/quick-fix.ts --all",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:quiet": "eslint . --ext .js,.jsx,.ts,.tsx --quiet",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "quality": "npm run type-check && npm run lint && npm run format:check",
    "quality:fix": "npm run type-check && npm run lint:fix && npm run format",
    "quality:omen": "npm run type-check:omen && npm run lint && npm run format:check",
    "prebuild": "npm run type-check",
    "prebuild:optimized": "npm run type-check",
    "prebuild:omen": "npm run type-check:omen",
    "test": "cross-env NODE_OPTIONS=--max-old-space-size=8192 jest --maxWorkers=6",
    "test:watch": "cross-env NODE_OPTIONS=--max-old-space-size=8192 jest --watch --maxWorkers=4",
    "test:coverage": "cross-env NODE_OPTIONS=--max-old-space-size=8192 jest --coverage --maxWorkers=6",
    "test:omen": "cross-env NODE_OPTIONS=--max-old-space-size=16384 jest --maxWorkers=10",
    "test:unit": "cross-env NODE_OPTIONS=--max-old-space-size=8192 jest src/__tests__/unit --maxWorkers=6",
    "test:integration": "cross-env NODE_OPTIONS=--max-old-space-size=8192 jest src/__tests__/unit --runInBand",
    "test:integration:db": "cross-env NODE_OPTIONS=--max-old-space-size=8192 jest --config jest.config.integration.js --runInBand",
    "test:integration:db:verbose": "cross-env NODE_OPTIONS=--max-old-space-size=8192 jest --config jest.config.integration.js --runInBand --verbose",
    "test:contracts": "cross-env NODE_OPTIONS=--max-old-space-size=8192 jest tests/contracts --runInBand",
    "test:contracts:stripe": "cross-env NODE_OPTIONS=--max-old-space-size=8192 jest tests/contracts/stripe --runInBand",
    "test:contracts:stripe:mock": "cross-env USE_STRIPE_MOCK=true jest tests/contracts/stripe --runInBand",
    "test:gpu": "cross-env NODE_OPTIONS=--max-old-space-size=8192 jest tests/performance/gpu-benchmark.test.ts --runInBand",
    "test:gpu:watch": "cross-env NODE_OPTIONS=--max-old-space-size=8192 jest tests/performance/gpu-benchmark.test.ts --watch --runInBand",
    "test:e2e": "node scripts/e2e-test.js --workers=6",
    "test:e2e:ui": "node scripts/e2e-test.js --ui",
    "test:e2e:headed": "node scripts/e2e-test.js --headed --workers=4",
    "test:e2e:debug": "node scripts/e2e-test.js --debug",
    "test:e2e:omen": "node scripts/e2e-test.js --workers=10",
    "test:e2e:direct": "playwright test --workers=6",
    "test:all": "npm run test && npm run test:e2e",
    "test:all:omen": "npm run test:omen && npm run test:e2e:omen",
    "test:full": "npm run test:unit && npm run test:integration:db && npm run test:e2e",
    "debug:auth": "tsx scripts/debug-nextauth.ts",
    "diagnose:api": "tsx scripts/diagnose-api-issue.ts",
    "start:all": "tsx scripts/start-server-and-bot.ts",
    "start:full-stack": "tsx scripts/start-full-stack.ts",
    "start:full-stack:verbose": "tsx scripts/start-full-stack.ts --verbose",
    "start:healing": "tsx scripts/start-full-stack.ts --no-server --no-bot",
    "server:bot": "tsx scripts/start-server-and-bot.ts",
    "server:bot:healing": "tsx scripts/start-full-stack.ts",
    "bot:check": "tsx scripts/website-checker-bot.ts",
    "bot:check:dev": "cross-env NEXT_PUBLIC_APP_URL=http://localhost:3001 tsx scripts/website-checker-bot.ts",
    "bot:watch": "tsx scripts/website-checker-bot.ts continuous",
    "bot:watch:dev": "cross-env NEXT_PUBLIC_APP_URL=http://localhost:3001 tsx scripts/website-checker-bot.ts continuous",
    "bot:run": "tsx scripts/workflow-monitor.ts all",
    "bot:critical": "tsx scripts/workflow-monitor.ts critical",
    "bot:health": "tsx scripts/workflow-monitor.ts health",
    "fix:e2e-tests": "tsx scripts/fix-e2e-tests.ts",
    "test:cursorrules": "tsx scripts/test-cursorrules-compliance.ts",
    "test:perplexity": "tsx scripts/testing/test-perplexity.ts",
    "test:perplexity:farming": "tsx scripts/testing/test-perplexity-farming.ts",
    "test:agents": "tsx scripts/test-agent-framework.ts",
    "test:orchestrator": "tsx scripts/test-workflow-orchestrator.ts",
    "test:orchestrator:demo": "tsx scripts/test-workflow-orchestrator.ts --demo",
    "test:orchestrator:all": "tsx scripts/test-workflow-orchestrator.ts --all",
    "test:remediation": "tsx scripts/test-auto-remediation.ts",
    "test:bridge": "tsx scripts/test-orchestrator-bridge.ts",
    "test:telemetry": "tsx scripts/test-telemetry.ts",
    "diagnostic": "tsx scripts/diagnostic-check.ts",
    "monitor": "tsx scripts/workflow-monitor.ts all",
    "monitor:all": "tsx scripts/workflow-monitor.ts all",
    "monitor:critical": "tsx scripts/workflow-monitor.ts critical",
    "monitor:health": "tsx scripts/workflow-monitor.ts health",
    "monitor:start": "tsx scripts/workflow-monitor.ts start",
    "monitor:list": "tsx scripts/workflow-monitor.ts list",
    "monitor:daemon": "tsx scripts/monitor-daemon.ts",
    "monitor:daemon:pm2": "pm2 start ecosystem.config.js --only workflow-monitor-daemon",
    "monitor:daemon:stop": "pm2 stop workflow-monitor-daemon",
    "monitor:daemon:restart": "pm2 restart workflow-monitor-daemon",
    "monitor:daemon:logs": "pm2 logs workflow-monitor-daemon",
    "monitor:daemon:status": "tsx scripts/check-daemon-status.ts",
    "monitor:dashboard:test": "tsx scripts/test-dashboard-apis.ts",
    "test:monitor": "tsx scripts/test-monitoring-bot.ts",
    "test:notifications": "tsx scripts/test-notifications.ts",
    "test:db-persistence": "tsx scripts/test-db-persistence.ts",
    "test:retry-system": "tsx scripts/test-retry-system.ts",
    "test:monitoring-integration": "tsx scripts/test-monitoring-integration.ts",
    "validate:24h": "tsx scripts/validate-24h.ts",
    "validate:status": "cat monitoring-reports/validation/validation-latest.json",
    "monitor:reports": "tsx scripts/workflow-monitor.ts reports",
    "monitor:workflow": "tsx scripts/workflow-monitor.ts workflow",
    "monitor:setup": "bash deployment/scripts/setup-monitoring.sh",
    "monitor:website": "tsx scripts/monitoring/enhanced-website-monitor.ts",
    "monitor:website:dev": "BASE_URL=http://localhost:3001 npm run monitor:website",
    "monitor:website:staging": "BASE_URL=https://staging.farmersmarket.com npm run monitor:website",
    "monitor:website:prod": "BASE_URL=https://farmersmarket.com npm run monitor:website",
    "type-check": "tsc --noEmit",
    "type-check:omen": "tsc --noEmit --maxNodeModuleJsDepth 10",
    "kill-server": "node scripts/kill-dev-server.js",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:seed:basic": "cross-env DATABASE_URL=\"postgresql://postgres:test_password_123@127.0.0.1:5433/farmersmarket_test\" tsx prisma/seed-basic.ts",
    "db:setup": "npm run db:push && npm run db:seed:basic",
    "db:reset": "prisma db push --force-reset && npm run db:seed:basic",
    "db:studio": "prisma studio",
    "db:test:setup": "tsx scripts/setup-test-db.ts",
    "vercel-build": "prisma generate && next build",
    "docker:build": "docker build -t farmers-market:latest -f docker/Dockerfile .",
    "docker:build-dev": "docker build -t farmers-market-dev:latest -f docker/Dockerfile.dev .",
    "docker:up": "docker-compose up -d",
    "docker:up-dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:up-build": "docker-compose up -d --build",
    "docker:up-build-dev": "docker-compose -f docker-compose.dev.yml up -d --build",
    "docker:down": "docker-compose down",
    "docker:down-dev": "docker-compose -f docker-compose.dev.yml down",
    "docker:down-volumes": "docker-compose down -v",
    "docker:logs": "docker-compose logs -f",
    "docker:logs-dev": "docker-compose -f docker-compose.dev.yml logs -f",
    "docker:logs-app": "docker-compose logs -f app",
    "docker:logs-db": "docker-compose logs -f postgres",
    "docker:restart": "docker-compose restart",
    "docker:restart-dev": "docker-compose -f docker-compose.dev.yml restart",
    "docker:restart-app": "docker-compose restart app",
    "docker:ps": "docker-compose ps",
    "docker:ps-dev": "docker-compose -f docker-compose.dev.yml ps",
    "docker:exec": "docker-compose exec app sh",
    "docker:exec-dev": "docker-compose -f docker-compose.dev.yml exec app sh",
    "docker:migrate": "docker-compose exec app npx prisma migrate deploy",
    "docker:migrate-dev": "docker-compose -f docker-compose.dev.yml exec app npx prisma migrate dev",
    "docker:seed": "docker-compose exec app npm run db:seed:basic",
    "docker:seed-dev": "docker-compose -f docker-compose.dev.yml exec app npm run db:seed:basic",
    "docker:prisma-studio": "docker-compose exec app npx prisma studio",
    "docker:db-shell": "docker-compose exec postgres psql -U farmers_user -d farmers_market",
    "docker:db-shell-dev": "docker-compose -f docker-compose.dev.yml exec postgres psql -U dev_user -d farmers_market_dev",
    "docker:test": "docker-compose -f docker-compose.dev.yml exec app npm test",
    "docker:test-e2e": "docker-compose -f docker-compose.dev.yml exec app npm run test:e2e",
    "docker:lint": "docker-compose -f docker-compose.dev.yml exec app npm run lint",
    "docker:clean": "docker-compose down -v && docker system prune -f",
    "docker:clean-all": "docker-compose down -v && docker system prune -a --volumes -f",
    "docker:health": "curl -f http://localhost:3000/api/health || echo 'Application not healthy'",
    "docker:health-db": "docker-compose exec postgres pg_isready -U farmers_user",
    "docker:health-redis": "docker-compose exec redis redis-cli ping",
    "docker:backup-db": "docker-compose exec postgres pg_dump -U farmers_user farmers_market > backup_$(date +%Y%m%d_%H%M%S).sql",
    "docker:stats": "docker stats"
  },
