{
  "name": "farmers-market-monorepo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cd farmers-market && npm run dev",
    "build": "cd farmers-market && npm run build",
    "build:safe": "cd farmers-market && npm run build:safe",
    "build:production": "cd farmers-market && npm run build:production",
    "start": "cd farmers-market && npm run start",
    "start:production": "cd farmers-market && npm run start:production",
    "lint": "cd farmers-market && npm run lint",
    "lint:fix": "cd farmers-market && npm run lint:fix",
    "test": "cd farmers-market && npm run test",
    "test:coverage": "cd farmers-market && npm run test:coverage",
    "test:e2e": "cd farmers-market && npm run test:e2e",
    "type-check": "cd farmers-market && npm run type-check",
    "install:all": "npm install && cd farmers-market && npm install",
    "clean": "cd farmers-market && rm -rf .next && rm -rf node_modules/.cache",
    "deploy:production": "cd farmers-market && npm run deploy:production"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-testing-library": "^7.13.1",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "playwright": "^1.56.0"
  }
}
