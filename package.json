{
  "name": "farmers-market",
  "version": "1.0.0",
  "private": true,
  "description": "Divine Agricultural E-Commerce Platform - Prisma 7",
  "engines": {
    "node": ">=20.19.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "postinstall": "prisma generate || true",
    "prepare": "husky",
    "dev": "cross-env NODE_OPTIONS='--max-old-space-size=16384' NODE_ENV=development next dev --turbo -p 3001",
    "dev:safe": "node scripts/start-dev-safe.js",
    "dev:webpack": "cross-env NODE_OPTIONS='--max-old-space-size=16384' NODE_ENV=development next dev -p 3001",
    "dev:turbo": "cross-env NODE_OPTIONS='--max-old-space-size=16384' NODE_ENV=development next dev --turbo -p 3001",
    "dev:omen": "cross-env NODE_OPTIONS='--max-old-space-size=32768 --max-semi-space-size=512' NODE_ENV=development next dev --turbo -p 3001",
    "dev:logger": "cross-env NODE_OPTIONS='--max-old-space-size=16384' NODE_ENV=development LOG_LEVEL=debug next dev --turbo -p 3001",
    "build": "cross-env NODE_OPTIONS='--max-old-space-size=16384' prisma generate && next build",
    "build:docker": "prisma generate && cross-env SKIP_ENV_VALIDATION=true next build",
    "build:optimized": "cross-env NODE_OPTIONS='--max-old-space-size=16384' prisma generate && next build",
    "build:omen": "cross-env NODE_OPTIONS='--max-old-space-size=32768 --max-semi-space-size=512' prisma generate && next build",
    "build:analyze": "cross-env ANALYZE=true NODE_OPTIONS='--max-old-space-size=16384' next build --webpack",
    "bundle:measure": "node scripts/measure-bundle-performance.mjs",
    "bundle:check": "npm run build:analyze && npm run bundle:measure",
    "bundle:validate": "npm run bundle:check",
    "start": "cross-env NODE_OPTIONS='--max-old-space-size=8192' next start -p 3001",
    "start:omen": "cross-env NODE_OPTIONS='--max-old-space-size=16384' next start -p 3001",
    "clean:cache": "rimraf .jest-cache coverage playwright-report",
    "clean:all": "rimraf .jest-cache coverage playwright-report node_modules/.cache",
    "cleanup:phase1": "bash scripts/cleanup-phase1.sh",
    "cleanup:phase2": "bash scripts/phase2-migrate-logging.sh",
    "audit:console": "bash scripts/audit-console-logs.sh",
    "audit:todo": "bash scripts/generate-todo-inventory.sh",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:quiet": "eslint . --ext .js,.jsx,.ts,.tsx --quiet",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "quality": "npm run type-check && npm run lint && npm run format:check",
    "quality:fix": "npm run type-check && npm run lint:fix && npm run format",
    "quality:omen": "npm run type-check:omen && npm run lint && npm run format:check",
    "prebuild": "npm run type-check",
    "prebuild:optimized": "npm run type-check",
    "prebuild:omen": "npm run type-check:omen",
    "test": "cross-env NODE_OPTIONS=--max-old-space-size=8192 jest --maxWorkers=6",
    "test:watch": "cross-env NODE_OPTIONS=--max-old-space-size=8192 jest --watch --maxWorkers=4",
    "test:coverage": "cross-env NODE_OPTIONS=--max-old-space-size=8192 jest --coverage --maxWorkers=6",
    "test:omen": "cross-env NODE_OPTIONS=--max-old-space-size=16384 jest --maxWorkers=10",
    "test:integration": "cross-env NODE_OPTIONS=--max-old-space-size=8192 jest src/__tests__/integration --runInBand",
    "test:gpu": "cross-env NODE_OPTIONS=--max-old-space-size=8192 jest tests/performance/gpu-benchmark.test.ts --runInBand",
    "test:gpu:watch": "cross-env NODE_OPTIONS=--max-old-space-size=8192 jest tests/performance/gpu-benchmark.test.ts --watch --runInBand",
    "test:e2e": "node scripts/e2e-test.js --workers=6",
    "test:e2e:ui": "node scripts/e2e-test.js --ui",
    "test:e2e:headed": "node scripts/e2e-test.js --headed --workers=4",
    "test:e2e:debug": "node scripts/e2e-test.js --debug",
    "test:e2e:omen": "node scripts/e2e-test.js --workers=10",
    "test:e2e:direct": "playwright test --workers=6",
    "test:all": "npm run test && npm run test:e2e",
    "test:all:omen": "npm run test:omen && npm run test:e2e:omen",
    "test:cursorrules": "tsx scripts/test-cursorrules-compliance.ts",
    "test:perplexity": "tsx scripts/testing/test-perplexity.ts",
    "test:perplexity:farming": "tsx scripts/testing/test-perplexity-farming.ts",
    "test:agents": "tsx scripts/test-agent-framework.ts",
    "test:telemetry": "tsx scripts/test-telemetry.ts",
    "diagnostic": "tsx scripts/diagnostic-check.ts",
    "monitor": "tsx scripts/workflow-monitor.ts all",
    "monitor:all": "tsx scripts/workflow-monitor.ts all",
    "monitor:critical": "tsx scripts/workflow-monitor.ts critical",
    "monitor:health": "tsx scripts/workflow-monitor.ts health",
    "monitor:start": "tsx scripts/workflow-monitor.ts start",
    "monitor:list": "tsx scripts/workflow-monitor.ts list",
    "monitor:daemon": "tsx scripts/monitor-daemon.ts",
    "monitor:daemon:pm2": "pm2 start ecosystem.config.js --only workflow-monitor-daemon",
    "monitor:daemon:stop": "pm2 stop workflow-monitor-daemon",
    "monitor:daemon:restart": "pm2 restart workflow-monitor-daemon",
    "monitor:daemon:logs": "pm2 logs workflow-monitor-daemon",
    "monitor:daemon:status": "tsx scripts/check-daemon-status.ts",
    "monitor:dashboard:test": "tsx scripts/test-dashboard-apis.ts",
    "test:monitor": "tsx scripts/test-monitoring-bot.ts",
    "test:notifications": "tsx scripts/test-notifications.ts",
    "test:db-persistence": "tsx scripts/test-db-persistence.ts",
    "test:retry-system": "tsx scripts/test-retry-system.ts",
    "test:monitoring-integration": "tsx scripts/test-monitoring-integration.ts",
    "validate:24h": "tsx scripts/validate-24h.ts",
    "validate:status": "cat monitoring-reports/validation/validation-latest.json",
    "monitor:reports": "tsx scripts/workflow-monitor.ts reports",
    "monitor:workflow": "tsx scripts/workflow-monitor.ts workflow",
    "monitor:setup": "bash deployment/scripts/setup-monitoring.sh",
    "monitor:website": "tsx scripts/monitoring/enhanced-website-monitor.ts",
    "monitor:website:dev": "BASE_URL=http://localhost:3001 npm run monitor:website",
    "monitor:website:staging": "BASE_URL=https://staging.farmersmarket.com npm run monitor:website",
    "monitor:website:prod": "BASE_URL=https://farmersmarket.com npm run monitor:website",
    "type-check": "tsc --noEmit",
    "type-check:omen": "tsc --noEmit --maxNodeModuleJsDepth 10",
    "kill-server": "node scripts/kill-dev-server.js",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:seed:basic": "cross-env DATABASE_URL=postgresql://postgres:postgres@localhost:5432/farmersmarket ts-node prisma/seed-basic.ts",
    "db:setup": "npm run db:push && npm run db:seed:basic",
    "db:reset": "prisma db push --force-reset && npm run db:seed:basic",
    "db:studio": "prisma studio",
    "db:test:setup": "tsx scripts/setup-test-db.ts",
    "vercel-build": "prisma generate && next build"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.0",
    "@auth/prisma-adapter": "^2.11.1",
    "@azure/monitor-opentelemetry-exporter": "^1.0.0-beta.28",
    "@azure/openai": "^2.0.0",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.2",
    "@langchain/core": "^0.3.0",
    "@langchain/openai": "^0.3.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.67.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.53.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.208.0",
    "@opentelemetry/instrumentation-http": "^0.53.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-node": "^0.208.0",
    "@opentelemetry/sdk-trace-base": "^1.28.0",
    "@opentelemetry/semantic-conventions": "^1.38.0",
    "@prisma/adapter-pg": "^7.0.0",
    "@prisma/client": "^7.0.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-toast": "^1.2.15",
    "@sentry/nextjs": "^10.26.0",
    "@stripe/react-stripe-js": "^5.4.0",
    "@stripe/stripe-js": "^8.5.2",
    "@tanstack/react-query": "^5.90.10",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-backend-webgl": "^4.22.0",
    "@tensorflow/tfjs-node": "^4.22.0",
    "@tensorflow/tfjs-node-gpu": "^4.22.0",
    "@types/pg": "^8.15.6",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "ai": "^5.0.0",
    "bcryptjs": "^3.0.3",
    "chalk": "^5.6.2",
    "cloudinary": "^2.8.0",
    "clsx": "^2.1.1",
    "commander": "^12.1.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.24",
    "jose": "^6.1.2",
    "lucide-react": "^0.554.0",
    "next": "16.0.3",
    "next-auth": "^5.0.0-beta.30",
    "next-intl": "^4.5.5",
    "next-themes": "^0.4.6",
    "nodemailer": "^7.0.10",
    "openai": "^4.77.0",
    "pg": "^8.16.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.66.1",
    "sharp": "^0.34.5",
    "sonner": "^2.0.7",
    "stripe": "^20.0.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@next/bundle-analyzer": "16.0.3",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/google.maps": "^3.58.1",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/nodemailer": "^7.0.4",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "baseline-browser-mapping": "^2.9.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.0.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "ioredis": "^5.8.2",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^27.2.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "prisma": "^7.0.1",
    "tailwindcss": "^3.4.18",
    "terser-webpack-plugin": "^5.3.14",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "glob": "^10.3.10",
    "inflight": "npm:noop2@^2.0.0",
    "js-yaml": "^4.1.1",
    "hono": "^4.10.6"
  }
}
