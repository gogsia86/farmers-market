    "jest-transform-stub": "^2.0.0",
    "jest-watch-select-projects": "^2.0.0",
    "jest-watch-suspend": "^1.1.2",
    "jest-watch-toggle-config": "^3.0.0",
    "jest-watch-typeahead": "^3.0.1",
    "jose": "^5.2.2",
    "lucide-react": "^0.331.0",
    "next": "14.2.33",
    "next-auth": "^5.0.0-beta.13",
